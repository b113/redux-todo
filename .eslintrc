{
    "plugins": [
        "react"
    ],
    "extends": "airbnb",
    "globals": {
        "document": true,
        "window": true,
        "localStorage": true
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-shadow": [
            "error",
            {
              "allow": ["addNote", "removeNote", "changeNote", "parseNotes"]
            }
          ]
        }

}